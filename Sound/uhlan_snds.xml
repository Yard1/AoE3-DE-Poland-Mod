<protounitsounddef>
  <protounit name="Uhlan">
    <soundtype name="Select">
      <civlogic>
        <choice name="Nature">
        </choice>
        <choice name="Russians">
          <soundset name="RussianCossackSelect">
          </soundset>
        </choice>
        <choice name="Germans">
          <soundset name="GermanUhlanSelect">
          </soundset>
        </choice>
      </civlogic>
    </soundtype>
    <soundtype name="Grunt">
      <soundset name="CavalryGrunt">
      </soundset>
    </soundtype>
    <soundtype name="Death">
      <soundset name="GenericMaleDeath">
      </soundset>
    </soundtype>
    <soundtype name="Creation">
      <soundset name="MilitaryBirth">
      </soundset>
    </soundtype>
    <soundtype name="Acknowledge">
      <civlogic>
        <choice name="Nature">
          <targetlogic>
            <choice name="default">
            </choice>
            <choice name="enemy">
            </choice>
          </targetlogic>
        </choice>
        <choice name="Russians">
          <targetlogic>
            <choice name="default">
              <soundset name="RussianCossackAcknowledge">
              </soundset>
            </choice>
            <choice name="enemy">
              <soundset name="RussianCossackAttack">
              </soundset>
            </choice>
          </targetlogic>
        </choice>
        <choice name="Germans">
          <targetlogic>
            <choice name="default">
              <soundset name="GermanUhlanAcknowledge">
              </soundset>
            </choice>
            <choice name="enemy">
              <soundset name="GermanUhlanAttack">
              </soundset>
            </choice>
          </targetlogic>
        </choice>
      </civlogic>
